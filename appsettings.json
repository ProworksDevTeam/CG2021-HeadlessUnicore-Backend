{
  "$schema": "https://json.schemastore.org/appsettings.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": ""
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": false
      },
      "Global": {
        "Id": "e996160a-e651-4c78-8e2f-57f3291f7d4f"
      },
      "ModelsBuilder": {
        "ModelsMode": "Nothing"
      }
    }
  },
  "GraphQL": {
    "Server": {
      "EnablePlayground": true,
      "EnableMetrics": true,
      "EnableCors": true,
      "CorsPolicyName": "GraphQLCors",
      "CorsAllowedHeaders": [ "*" ],
      "CorsAllowedMethods": [ "*" ],
      "CorsAllowedOrigins": [ "*" ]
    }
  }
}